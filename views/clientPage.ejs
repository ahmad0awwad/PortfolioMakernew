        <!-- Display logo -->
        <!-- <div class="logo">
            <img src="<%= client.logoUrl %>" alt="Logo of <%= client.name %>">
        </div> -->
   

        <div class="himage">
            <img src="public/images/aqsa.jpg" alt="Banner Image">
          </div>
    
    

        <!-- Page Title -->
        <p class="page-title"><%= client.name %></p>
        <p class="section-subheading"><%= client.slogan  %></p>
        <h2 class="intro">
            <%= client.Bio  %>
        </h2>
        <div class="hr">
            <hr>
        </div>
    
        <!-- Portfolio Section -->
        <p class="ma">Portfolio</p>
        <div class="container">
            <div class="row row-cols-1 row-cols-md-2 row-cols-lg-3 g-4">
                <% if (client.projects && client.projects.length > 0) { %>
                    <% client.projects.forEach((project) => { %>
                        <div class="col bm-2">
                            <div class="card">
                                <img src="<%= project.imageUrl %>" alt="<%= project.title || 'Project Image' %>" class="card-img">
                                <div class="card-body">
                                    <h5 class="card-title"><%= project.title || 'Untitled Project' %></h5>
                                    <p class="card-text"><%= project.description || 'No description available.' %></p>
                                    <a href="<%= project.link || '#' %>" target="_blank" class="btn btn-primary">
                                        <%= project.cta || 'View Details' %>
                                    </a>
                                </div>
                            </div>
                        </div>
                    <% }); %>
              
                <% } %>
            </div>
        </div>
    
        <!-- Fun Section -->
        <div class="applet-div">
            <p class="section-heading">Take a Break and  laugh!</p>
        
            <form id="query-form">
                <div>
                    <label for="animal1">Enter the name of an Animal:</label> <input class="textfield" id="animal1" type="text" name="animal1" placeholder="e.g. cat">
                    </div>
                    <div>
                    <label for="animal">Enter the name of an Animal:</label> <input class="textfield" id="animal" type="text" name="animal" placeholder="e.g. dolphin">
                    </div>     
                    <div class="button">
                    <div class="poetry-title" id="poetry-title"></div>
                    <div class="poetry-body" id="poetry-body"></div>
        
                    <input class="btn btn-warning" type="button" value="Reset" id="resetButton" onclick="this.form.reset();resetResults()">
                    <input class="btn btn-primary" type="submit" value="Make / Remake" id="makeButton">
        
                    <div class="poetry-form-message" id="form-message"></div>
                </div>
            </form>
            <br>
            <p data-toggle="tooltip" data-placement="bottom" title="" onclick="javascript:toolTipReset()" data-original-title="These tags tell you what technologies I use">
              Powered by :
              <span class="keyword-magnet">JavaScript</span>
              <span class="keyword-magnet">JQuery</span>
              <span class="keyword-magnet">Bootstrap</span>
          </p>
          </div>
          <div class="hr">
            <hr>
          </div>
    
        <!-- Skill Summary -->
        <div class="container">
            <p class="ma">Skill Summary</p>
            <p>
                <div id="skills-container">
                    <% if (client.skills && client.skills.length > 0) { %>
                        <% client.skills.forEach(skill => { %>
                            <span class="skill-item"><%= skill %></span>
                        <% }); %>
                    <% } else { %>
                        <p>No skills added yet.</p>
                    <% } %>
                </div>
                
            </p>
          
        </div>
    
        <div class="hr">
            <hr>
        </div>
    
        <!-- Education Section -->
<p class="ma">Education</p>
<% client.education.forEach(edu => { %>
    <p class="section-subheading"><%= edu.institution %></p>
    <p class="section-small-heading"><%= edu.degree %></p>
<% }); %>


        <div class="hr">
            <hr>
        </div>
    
      <!-- Social Media Links -->
      <div class="social-icons">
        <% if (client.socialMedia && client.socialMedia.length > 0) { %>
            <% client.socialMedia.forEach(media => { %>
                <div class="social-media-item">
                    <img src="<%= media.icon %>" alt="<%= media.name %>" class="icon">
                    <a class="Cont" href="<%= media.url %>" target="_blank" onclick="handleContact('<%= media.name %>')">
                        <%= media.url %>
                    </a>
                </div>
            <% }); %>
        <% } %>
    </div>
    
    

        <div class="hr">
            <hr>
        </div>
    
        <!-- Hero Image -->
        <div class="himage">
            <img src="public/images/petra.jpg" alt="Banner Image">
          </div>
    
        <!-- Footer -->
        <!-- <footer class="footer">
            <p>© Copyright 2024 <%= client.name || 'Ahmad Awwad' %></p>
        </footer> -->
    </div>
      
    
</body>
</html>
